{
    "name": "Chizuru",
    "description": "A Normal Telegram Bot Which can Download And Rename Your Files",
    "logo": "https://wallpapercave.com/wp/wp6965282.jpg",
    "keywords": ["Telegram","python"],
    "repository": "https://github.com/The-Hunter-Oraganization/Chizuru",
    "formation": {
      "worker": {
        "quantity": 1,
        "size": "free"
      }
    },
    "env": {
      "API_ID": {
        "description": "App API_ID from https://my.telegram.org"
      },
      "API_HASH": {
        "description": "App APP_HASH from https://my.telegram.org"
      },
      "BOT_TOKEN": {
        "description": "Bot token from https://t.me/BotFather",
        "required": true
      },
      "OWNER": {
        "description": "Your Telegram ID",
        "required": true
      },
      "MONGO_DB": {
        "description": "get from cloud.mongodb.com",
        "required": true
      }
    },
}